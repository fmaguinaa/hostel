{
  "env": {
    "development": {
      "presets": [
        "next/babel"
      ],
      "plugins": [
        [
          "import",
          {
            "libraryName": "antd",
            "style": "css"
          }
        ],
        [
          "styled-components",
          {
            "ssr": true,
            "displayName": true,
            "preprocess": false
          }
        ],
        ["module-resolver", {
          "root": ["./"],
          "alias": {
            "components": "./src/components",
            "containers": "./src/containers"
          }
        }]
      ]
    },
    "production": {
      "presets": [
        "next/babel"
      ],
      "plugins": [
        [
          "import",
          {
            "libraryName": "antd",
            "style": "css"
          }
        ],
        [
          "styled-components",
          {
            "ssr": true,
            "displayName": true,
            "preprocess": false
          }
        ],
        ["module-resolver", {
          "root": ["./"],
          "alias": {
            "components": "./src/components",
            "containers": "./src/containers"
          }
        }]
      ],
    },
    "test": {
      "presets": ["react", "env", "stage-0"],
      "plugins": [
        "require-context-hook",
        [
          "import",
          {
            "libraryName": "antd",
            "style": "css"
          }
        ],
        [
          "styled-components",
          {
            "ssr": true,
            "displayName": true,
            "preprocess": false
          }
        ],
        ["module-resolver", {
          "root": ["./"],
          "alias": {
            "components": "./src/components",
            "containers": "./src/containers"
          }
        }]
      ]
    }
  }
}

